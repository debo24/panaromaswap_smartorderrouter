{
  "name": "panaromaswap_smartorderrouter",
  "version": "1.0.13",
  "description": "Panaromaswap Smart Order Router",
  "main": "build/main/index",
  "typings": "build/main/index",
  "module": "build/module/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/debo24/panaromaswap_smartorderrouter.git"
  },
  "publishConfig": {
    "@debo24": "https://npm.pkg.github.com"
  },
  "keywords": [],
  "scripts": {
    "compile-v3-types": "npx typechain --target ethers-v5 --out-dir src/types/v3 './node_modules/?(panaromaswap_v3core|panaromaswap_v3periphery)/artifacts/contracts/**/*.json'",
    "compile-v2-types": "npx typechain --target ethers-v5 --out-dir src/types/v2 './node_modules/?(panaromaswap_v2core|panaromaswap_v2periphery)/build/*PanaromaswapV2*.json'",
    "compile-router": "npx typechain --target ethers-v5 --out-dir src/types/other './node_modules/panaromaswap_swaproutercontracts/artifacts/contracts/**/*.json'",
    "compile-external-types": "npx typechain --target ethers-v5 --out-dir src/types/other 'src/abis/**/*.json'",
    "build": "run-p compile-v3-types compile-v2-types compile-router compile-external-types && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "version": "standard-version",
    "prepare-release": "run-s cov:check doc:html version doc:publish",
    "test": "jest test/unit",
    "integ-test": "TS_NODE=true jest test/integ"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@types/async-retry": "^1.4.2",
    "@types/await-timeout": "^0.3.1",
    "@types/bunyan": "^1.8.6",
    "@types/bunyan-blackhole": "^0.2.2",
    "@types/lodash": "^4.14.168",
    "@types/stats-lite": "^2.2.0",
    "panaromaswap_defaulttokenlist": "1.0.1",
    "panaromaswap_routersdk": "1.0.4",
    "panaromaswap_swaproutercontracts": "1.0.0",
    "panaromaswap_tokenlists": "1.0.0",
    "panaromaswap_v2sdk": "1.0.5",
    "panaromaswap_v3sdk": "1.0.5",
    "async-retry": "^1.3.1",
    "await-timeout": "^1.1.1",
    "axios": "^0.21.1",
    "bunyan": "^1.8.15",
    "bunyan-blackhole": "^1.1.1",
    "bunyan-debug-stream": "^2.0.0",
    "ethers": "^5.6.1",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "hardhat": "^2.9.6",
    "lodash": "^4.17.21",
    "mnemonist": "^0.38.3",
    "node-cache": "^5.1.2",
    "stats-lite": "^2.2.0"
  },
  "devDependencies": {
    "@ethersproject/providers": "^5.4.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@oclif/command": "^1.8.0",
    "@oclif/errors": "^1.3.5",
    "@typechain/ethers-v5": "^7.0.1",
    "@types/jest": "^27.0.0",
    "@types/jest-sinon": "^1.0.2",
    "@types/node": "^15.0.2",
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "panaromaswap_v2core": "1.0.1",
    "panaromaswap_v2periphery": "1.0.1",
    "panaromaswap_v3core": "1.0.0",
    "panaromaswap_v3periphery": "1.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^27.0.6",
    "jest-environment-hardhat": "^1.1.8",
    "jest-expect-message": "^1.0.2",
    "jest-mock": "^27.0.6",
    "jest-sinon": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "open-cli": "^6.0.1",
    "prettier": "^2.1.1",
    "prettier-plugin-organize-imports": "^2.1.0",
    "sinon": "^11.1.2",
    "standard-version": "^9.0.0",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.8.1",
    "typechain": "^5.0.0",
    "typescript": "^4.2.2"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "prettier": {
    "singleQuote": true
  },
  "oclif": {
    "commands": "./cli/commands"
  }
}
